{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es6"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "types": ["jest"],
    "jsx": "react",
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {"react-native": ["."]},
    "esModuleInterop": true
  }
}
